.layui-form-radio>i:hover, .layui-form-radioed>i {color: #409EFF;}
.forecast {overflow-x : hidden;overflow-y : hidden;display: flex;}
.forecast .left{min-width: 320px;min-height: 700px;}
.forecast .main{background-color: rgb(246, 246, 246);width: 100%;}
.forecast .right{min-width: 300px;}
.forecast .main-plane{}
.forecast .main-section{display: none;}
.forecast .main-temporal{display: none;}
.forecast .main-sounding{display: none;}


.left .layui-tab {margin: 0;}
.left .layui-tab-bar {display: none;}
.left .layui-tab .layui-tab-title li {width: 25%;padding: 0;}
.left .layui-tab .layui-tab-content {}
.left .layui-tab .layui-tab-content .factor-box .factor-num{display: flex;}
.left .layui-tab .layui-tab-content .factor-box .factor-num .icon{display: flex;justify-content: center;align-items: center;border-radius: 25px;background-color: #EBEEF5;color: #999999;width: 20px;height: 20px;font-size: 12px;}
.left .layui-tab .layui-tab-content .factor-box .factor-num ul{display: flex;width: calc(100% - 40px);}
.left .layui-tab .layui-tab-content .factor-box .factor-num ul li{width: 33%;text-align: center;color: #409EFF;}
.left .layui-tab .layui-tab-content .factor-box .factor-item{display: flex;justify-content: space-around;margin-top: 6px;text-align: center;}
.left .layui-tab .layui-tab-content .factor-box .factor-item li {margin-top: 4px;}
.left .layui-tab .layui-tab-content .but-row{margin: 6px;display: flex;}
.left .layui-tab .layui-tab-content .but-row .is-active{}
.left .layui-tab .layui-tab-content .but-row .button-item{margin: 4px;padding: 0 3px;min-width: 60px;text-align: center;}

.right .layui-tab {margin: 0;}
.right .layui-tab .layui-tab-content .but-row{margin: 6px;display: flex;}
.right .layui-tab .layui-tab-content .but-row .button-item{margin: 4px;min-width: 50px;text-align: center;}

.main .tools{display: flex;height: 41px;line-height: 41px;border-bottom: 1px solid #e6e6e6;}
.main .tools .model{margin-left: 40px;display: flex;}
.main .tools .model ul{display: flex;align-items: center;}
.main .tools .model ul li{margin: 0 8px;padding: 5px;}
.main .tools .model ul .is-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px;height: 10px;display: flex;justify-content: center;align-items: center;}

.main .tools .scope{margin-left: 40px;display: flex;}
.main .tools .scope ul{display: flex;align-items: center;}
.main .tools .scope ul li{margin: 0 8px;padding: 5px;}
.main .tools .scope ul .is-active{color: #409EFF;}

.main {position: relative;}
.main .content .page-but{display: flex;justify-content: center;padding: 20px 0;position: relative;height: 50px;}
.main .content .page-but .but-right{position: absolute;right:20px;height: 50px;display: flex;align-items: center;}
.main .content .page-but .but-right .but-txt{font-size: 14px;color: #909399;padding: 5px 10px;margin: 0 10px;border-radius: 100px;border: #999999 1px solid;min-width: 30px;text-align: center;}
.main .content .content-map {display: flex;justify-content: center;text-align: center;}
.main .content .content-map .browse .title{display: flex;justify-content: center;font-size: 24px;padding: 15px 0;}
.main .content .content-map .stamps .title{display: flex;justify-content: center;font-size: 24px;padding: 15px 0;}
.main .play {height: 40px;background: rgba(0,0,0,0.25);;display: flex;justify-content: center;align-items: center;position: absolute;bottom: 0px;width: 100%;}

.main .content .stamps-box {display: none;}
.main .content .content-map .stamps{display: flex;flex-wrap: wrap;padding: 0px 80px 40px 80px;}
.main .content .content-map .stamps .col-item {width: 50%;}
.main .content .content-map .stamps .col-item .item-title{height: 40px;display: flex;justify-content: center;align-items: center;font-size: 20px;}
.main .content .content-map .stamps .col-item .item-map-img{padding: 0px 40px 20px 40px}
.main .content .content-map .stamps .col-item .item-map-img img{width: 100%;height: 300px;}

.main .play .play-but {display: flex;}
.main .play .play-but .but-item{display: flex;color: #999; min-width: 50px;height: 30px;border-radius: 15px;background: #fff;margin: 0 10px;justify-content: center;align-items: center;}
.main .play .play-but .is-active{background: #1E9FFF;color: #fff;}
.main .play .on-off .layui-form {display: flex;align-items: center;}
.main .play .on-off .layui-form span{margin: 0 12px;}
.main .play .on-off .layui-form-switch {margin: 0;}
.main .play .on-off .layui-form-onswitch{border-color: #409EFF; background-color: #409EFF;}
.main .play .on-off .fast {color:#419FFF;}

.left .temporal .floor-factor .but-row{text-align: center;display: flex;align-content: flex-start;flex-flow: row wrap;}
.left .temporal .floor-factor .but-row .item-checkbox{flex: 0 0 33%;}
.left .temporal .floor-factor .but-row .layui-form-checkbox{padding-bottom: 10px;}
.left .temporal .floor-factor .but-row .layui-form-checked i{background: #419FFF;border-color: #419FFF!important;}

.left .temporal .controls .layui-input {height: auto;}

.left .sounding .controls .layui-input {height: auto;}

.right #forecastTime .forecast-time-active{color:#419FFF;}
.right #forecastAfter .after-time-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px}
.right #forecastFrequency .frequency-time-active{color:#419FFF;}
.left #groundFactor .ground-factor-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px}
.left #temporalGround .temporal-factor-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px}
.left #upperAir .upper-factor-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px}
.left #synthesize .synthesize-time-active{color: cornsilk;background-color: #1E9FFF;}
.left #temporalSite .temporal-time-active{color: cornsilk;background-color: #1E9FFF;}
.left #soundingSite .sounding-time-active{color: cornsilk;background-color: #1E9FFF;}
.left #sectionSynthesize .sectionSynthesize-time-active{color: cornsilk;background-color: #1E9FFF;}
.tools #cityArr .city-time-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px;height: 10px;display: flex;justify-content: center;align-items: center;}

.left #synthesize .synthesize{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;padding: 3px 0px 3px 5px;}
.left #temporalSite .temporalSite{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.left #soundingSite .soundingSite{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.left #sectionSynthesize .sectionSynthesize{overflow: hidden;text-overflow: ellipsis;white-space: nowrap;padding: 3px 0px 3px 5px;}

.left .section #sectionSite {text-align: center;display: flex;align-content: flex-start;flex-flow: row wrap;}
.left .section #sectionSite li{flex: 0 0 33%;margin: 3px 0;}
.left .section #sectionSite .site-time-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px;height: 20px}

.left .temporal #temporalSite {text-align: center;display: flex;align-content: flex-start;flex-flow: row wrap;}
.left .temporal #temporalSite li{flex: 0 0 33%;margin: 3px 0;}
.left .temporal #temporalSite .temporal-time-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px;height: 20px}

.left .sounding #soundingSite {text-align: center;display: flex;align-content: flex-start;flex-flow: row wrap;}
.left .sounding #soundingSite li{flex: 0 0 33%;margin: 3px 0;}
.left .sounding #soundingSite .sounding-time-active{color: cornsilk;background-color: #1E9FFF;border-radius: 100px;height: 20px}
